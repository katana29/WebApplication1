@model WebApplication1.Models.LoginViewModel
@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<!-- Head -->
<head>

    <title>GETON LOGIN</title>

    <!-- Meta-Tags -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">

    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
  
    <!-- Style -->
    <link rel="stylesheet" href="/newlogin/css/style.css" type="text/css" media="all">

    <!-- Fonts -->
    <link href="//fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900" rel="stylesheet">
   

</head>
<!-- //Head -->
<!-- Body -->
<body>


    </br></br></br></br></br></br></br></br></br>
    <h2>Log in to your Account</h2>
    <div class="w3layoutscontaineragileits">

        @using (Html.BeginForm("NewLogin", "Account", FormMethod.Post))
        {
            @Html.AntiForgeryToken()
            @Html.TextBoxFor(m => m.UserName, new { @placeholder = "Username" })
            @Html.PasswordFor(m => m.Password, new { @placeholder = "Password" })
            <ul class="agileinfotickwthree">
                <li>
                    @Html.CheckBoxFor(m => m.RememberMe)
                    <label for="brand1"><span></span>Remember me</label>
                </li>
            </ul>
            <div class="aitssendbuttonw3ls">
                <input type="submit" value="LOGIN">

                <div class="clear"></div>
            </div>
        }


    </div>
    <div class="w3footeragile">
        <p> &copy; 2021 GETON Login Form. All Rights Reserved | by Ana Katic </p>
    </div>

</body>


</html>
